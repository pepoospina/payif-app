{
  "name": "functions",
  "version": "0.1.0",
  "scripts": {
    "test": "NODE_ENV=test FIRESTORE_EMULATOR_HOST=127.0.0.1:8080 mocha --config .mocharc.json",
    "lint": "eslint --ext .js,.ts .",
    "build": "tsc",
    "build:watch": "rm -rf ./lib & tsc --watch",
    "build:watch-test": "rm -rf ./lib-test & tsc --watch -p ./tsconfig.test.json",
    "build:watch-scripts": "tsc --watch -p ./tsconfig.scripts.json",
    "build:watch-migration": "tsc --watch -p ./tsconfig.migration.json",
    "build:bsky-client": "tsc --watch -p ./local-client/tsconfig.json",
    "serve:bind-port": "sudo iptables -t nat -F & sudo iptables -t nat -A PREROUTING -p tcp --dport 80 -j REDIRECT --to-port 3100 & sudo iptables -t nat -A OUTPUT -p tcp -o lo --dport 80 -j REDIRECT --to-port 3100",
    "serve:bsky-client-port": "ts-node ./local-client/src/index.ts",
    "serve:bsky-client": "authbind --deep yarn serve:bsky-client-port",
    "serve": "npm run build && firebase emulators:start --only functions",
    "shell": "npm run build && firebase functions:shell",
    "start": "npm run shell",
    "deploy": "firebase deploy --only functions",
    "run-script": "ts-node --project ./tsconfig.scripts.json",
    "run-migration": "ts-node --project ./tsconfig.migration.json",
    "add-accounts": "yarn run-script ./scripts/016-add-account-data.ts",
    "logs": "firebase functions:log",
    "run-seed": "ts-node --project ./tsconfig.scripts.json"
  },
  "engines": {
    "node": "20"
  },
  "main": "lib/index.js",
  "dependencies": {
    "@atproto/api": "^0.14.9",
    "@atproto/jwk-jose": "^0.1.7",
    "@atproto/oauth-client-node": "^0.2.23",
    "@clerk/express": "^1.7.5",
    "@types/node-forge": "^1.3.11",
    "@types/pino": "^7.0.5",
    "algoliasearch": "5",
    "cors": "^2.8.5",
    "crypto": "^1.0.1",
    "crypto-js": "^4.2.0",
    "date-fns": "^4.1.0",
    "express": "^4.18.2",
    "fetch-sparql-endpoint": "^4.1.1",
    "firebase-admin": "^12.1.1",
    "firebase-functions": "^6.4.0",
    "googleapis": "^150.0.1",
    "jsonwebtoken": "^9.0.2",
    "masto": "^6.8.0",
    "metascraper": "^5.49.1",
    "metascraper-description": "^5.49.1",
    "metascraper-image": "^5.49.1",
    "metascraper-title": "^5.49.1",
    "metascraper-url": "^5.49.1",
    "node-fetch": "^3.3.2",
    "node-forge": "^1.3.1",
    "oauth-1.0a": "^2.2.6",
    "postmark": "^4.0.4",
    "sharp": "^0.34.2",
    "stripe": "^18.0.0",
    "twitter-api-v2": "^1.23.2",
    "viem": "^2.7.10",
    "yup": "^1.3.3"
  },
  "devDependencies": {
    "@johanblumenberg/ts-mockito": "^1.0.41",
    "@types/chai": "^4.3.4",
    "@types/jest": "^29.5.12",
    "@types/mocha": "^10.0.1",
    "@types/sinon": "^17.0.3",
    "@types/supertest": "^6.0.2",
    "@typescript-eslint/eslint-plugin": "^5.12.0",
    "@typescript-eslint/parser": "^5.12.0",
    "chai": "^4.3.7",
    "dotenv": "^16.4.5",
    "firebase-functions-test": "^3.1.0",
    "mocha": "^10.2.0",
    "prettier": "^3.3.3",
    "prettier-plugin-organize-imports": "^4.2.0",
    "puppeteer": "^24.10.0",
    "sinon": "^18.0.0",
    "supertest": "^6.3.4",
    "ts-mockito": "^2.6.1",
    "ts-node": "^10.9.1",
    "typescript": "^5.3.3"
  },
  "private": true
}
